#################################\n","import pandas as pd\n","\n","# Make a copy to preserve original DataFrame\n","df = df_exoplanets[['pl_name', 'st_mass','st_rad','pl_orbper', 'pl_trandur','pl_trandep', 'log_orbital_period', 'log_trandur', 'log_trandep']].copy()\n","\n","# Columns to process\n","cols = ['log_orbital_period', 'log_trandur', 'log_trandep', 'st_mass','st_rad']\n","\n","# Track removed row indices and statistics\n","removed_indices = set()\n","outlier_stats = {}\n","\n","for col in cols:\n","    q1 = df[col].quantile(0.25)\n","    q3 = df[col].quantile(0.75)\n","    iqr = q3 - q1\n","    lower_whisker = q1 - 1.5 * iqr\n","    upper_whisker = q3 + 1.5 * iqr\n","\n","    # Identify outliers based on whisker rule\n","    below = df[df[col] < lower_whisker]\n","\n","    # Only remove above-whisker outliers for columns other than 'log_trandep'\n","    if col == 'log_trandep':\n","        above = pd.DataFrame(columns=df.columns)  # empty dataframe\n","    else:\n","        above = df[df[col] > upper_whisker]\n","\n","    #above = df[df[col] > upper_whisker]\n","\n","    # Add their indices to the set\n","    removed_indices.update(below.index)\n","    removed_indices.update(above.index)\n","\n","    # Store statistics\n","    outlier_stats[col] = {\n","        'lower_whisker': lower_whisker,\n","        'upper_whisker': upper_whisker,\n","        'removed_below': len(below),\n","        'removed_above': len(above),\n","        'total_removed_for_col': len(below) + len(above)\n","    }\n","\n","# Remove all unique outlier rows\n","df_filtered = df.drop(index=removed_indices)\n","\n","# Final summary\n","print(f\"Original number of rows: {len(df)}\")\n","print(f\"Number of rows after removing outliers: {len(df_filtered)}\")\n","print(f\"Total number of unique records removed: {len(removed_indices)}\\n\")\n","\n","# Print per-column stats\n","for col, stats in outlier_stats.items():\n","    print(f\"Column: {col}\")\n","    print(f\"  Lower whisker: {stats['lower_whisker']:.4f}\")\n","    print(f\"  Upper whisker: {stats['upper_whisker']:.4f}\")\n","    print(f\"  Records removed below lower whisker: {stats['removed_below']}\")\n","    print(f\"  Records removed above upper whisker: {stats['removed_above']}\")\n","    print(f\"  Total removed for column (not deduplicated): {stats['total_removed_for_col']}\\n\")\n"],"metadata":{"id":"q1BHfR_r-gyZ","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1752044882422,"user_tz":240,"elapsed":25,"user":{"displayName":"Prem Isaac","userId":"04442162912535875359"}},"outputId":"e5f5c3e0-f0e8-485c-82e8-cfd5dae6f60b"},"execution_count":28,"outputs":[{"output_type":"stream","name":"stdout","text":["Original number of rows: 1424\n","Number of rows after removing outliers: 1308\n","Total number of unique records removed: 116\n","\n","Column: log_orbital_period\n","  Lower whisker: -0.5013\n","  Upper whisker: 2.5633\n","  Records removed below lower whisker: 0\n","  Records removed above upper whisker: 11\n","  Total removed for column (not deduplicated): 11\n","\n","Column: log_trandur\n","  Lower whisker: -0.2026\n","  Upper whisker: 1.2738\n","  Records removed below lower whisker: 3\n","  Records removed above upper whisker: 4\n","  Total removed for column (not deduplicated): 7\n","\n","Column: log_trandep\n","  Lower whisker: -2.5421\n","  Upper whisker: -0.1946\n","  Records removed below lower whisker: 2\n","  Records removed above upper whisker: 0\n","  Total removed for column (not deduplicated): 2\n","\n","Column: st_mass\n","  Lower whisker: 0.5275\n","  Upper whisker: 1.3875\n","  Records removed below lower whisker: 25\n","  Records removed above upper whisker: 46\n","  Total removed for column (not deduplicated): 71\n","\n","Column: st_rad\n","  Lower whisker: 0.3431\n","  Upper whisker: 1.5861\n","  Records removed below lower whisker: 7\n","  Records removed above upper whisker: 75\n","  Total removed for column (not deduplicated):